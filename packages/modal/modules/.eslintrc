{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": ["import", "react"],
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended"
  ],
  "rules": {
    "prefer-arrow-callback": 2,
    "react/display-name": 0,
    "react/no-deprecated:": 0
  },
  "settings": {
        "react": {
            "version": "0.14.5"
        },
        "propWrapperFunctions": [
            "forbidExtraProps"
        ]
    }
}
